{
  "name": "blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build -- --prod",
    "clean": "rm -rf dist && rm -f src/assets/scully-routes.json",
    "test": "ng test",
    "test:serve": "npm run test -- --watch-all",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "npx scully --",
    "scully:serve": "npx scully serve --",
    "scully:full-build": "npm run clean && npm run build && npm run scully -- --scanRoutes",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.7",
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/forms": "~11.2.7",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "@briebug/jest-schematic": "^3.1.0",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@scullyio/init": "^1.1.2",
    "@scullyio/ng-lib": "^1.0.0",
    "@scullyio/scully": "^1.0.0",
    "prismjs": "^1.23.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.1.1",
    "@angular-devkit/build-angular": "~0.1102.6",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@ngneat/spectator": "^7.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/jest": "26.0.22",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "husky": "^5.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "26.6.3",
    "karma-coverage": "~2.0.3",
    "lint-staged": "^10.5.4",
    "ng-mocks": "^11.10.0",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
