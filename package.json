{
  "name": "blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build -- --prod",
    "clean": "rm -rf dist && rm -f src/assets/scully-routes.json",
    "test": "ng test",
    "test:serve": "npm run test -- --watch-all",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "npx scully",
    "scully:scan-routes": "npm run scully -- --scanRoutes",
    "scully:serve": "npx scully serve",
    "scully:full-build": "npm run clean && npm run build && npm run scully:scan-routes",
    "prepare": "husky install",
    "dist": "npm run clean && npm run build:prod && npm run scully -- --scanRoutes"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.7",
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/forms": "~11.2.7",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "@briebug/jest-schematic": "^3.1.0",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@scullyio/init": "^1.1.2",
    "@scullyio/ng-lib": "^1.0.0",
    "@scullyio/scully": "^1.0.0",
    "fuse.js": "^6.4.6",
    "prismjs": "^1.23.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.1.1",
    "@angular-devkit/build-angular": "~0.1102.6",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@ngneat/spectator": "^7.1.0",
    "@types/jest": "26.0.22",
    "@types/node": "^14.16",
    "@types/sharp": "^0.28.2",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^5.2.0",
    "jest": "26.6.3",
    "jest-createspyobj": "^2.0.0",
    "jest-marbles": "^2.5.1",
    "lint-staged": "^10.5.4",
    "ng-mocks": "^11.10.0",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "scully-plugin-time-to-read": "0.0.4",
    "sharp": "^0.28.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
